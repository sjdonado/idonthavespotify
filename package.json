{
  "name": "idonthavespotify",
  "version": "1.7.3",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bun run watch:assets\" \"bun run watch:server\"",
    "watch:assets": "bun run build.config.ts --watch",
    "watch:server": "bun run --watch src/index.ts",
    "build": "bun run build.config.ts --minify",
    "build:prod": "bun build src/index.ts --compile --outfile ./dist/idonthavespotify",
    "test": "NODE_ENV=test bun test",
    "test:mocks:fetch": "bun run scripts/fetch-snapshots.ts",
    "test:ci": "bun run test:mocks:fetch && NODE_ENV=test bun test",
    "lint": "eslint ./src",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "@umami/node": "^0.4.0",
    "axios": "^1.7.9",
    "axios-retry": "^4.5.0",
    "cheerio": "^1.0.0",
    "howler": "^2.2.4",
    "html-entities": "^2.6.0",
    "nano-jsx": "^0.2.0",
    "notyf": "^3.10.0",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "string-similarity": "^4.0.4",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@tailwindcss/cli": "4.1",
    "@types/bun": "^1.2.5",
    "@types/howler": "^2.2.12",
    "@types/string-similarity": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "axios-mock-adapter": "^2.1.0",
    "concurrently": "^9.2.1",
    "domhandler": "^5.0.3",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx": "^0.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.3.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "4.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.38.0"
  }
}
